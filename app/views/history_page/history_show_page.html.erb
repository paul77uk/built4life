<% @history_day.each do |day| %>
  <div class="text-2xl font-semibold text-gray-700"><%= day.date %></div>
  <% day.history_exercises.each do |exercise| %>
    <div class="text-1xl font-semibold text-gray-700"><%= exercise.title %></div>
    <% exercise.history_sets.each do |history_set| %>
      <div><%= history_set.exercise_set %> <%= history_set.weight %> <%= history_set.reps_dist %></div>
    <% end %>
  <% end %>
<% end %>
